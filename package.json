{
  "name": "eslint-config-mxo",
  "description": "ESLint shareable config for XO with 2-space indent and some other sane tweaks",
  "version": "1.0.0",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:forivall/eslint-config-mxo.git"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "author": "Emily Marigold Klassen <forivall@gmail.com>",
  "keywords": [
    "❤️",
    "space",
    "spaces",
    "eslintconfig",
    "xo",
    "xoxo",
    "hugs",
    "lint",
    "linter",
    "jshint",
    "jslint",
    "eslint",
    "validate",
    "code style",
    "standard",
    "check",
    "checker",
    "verify",
    "hint"
  ],
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "posttest": "npm run lint",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "eslint -c ./full/index.js --ignore-pattern test .",
    "lint:test": "eslint -c ./full/ava.js --parser-options=ecmaVersion:6,sourceType:module test"
  },
  "files": [
    "index.js",
    "standalone.js",
    "ava.js"
  ],
  "dependencies": {
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-promise": "^4.2.1"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^3.1.1",
    "is-plain-obj": "^2.0.0",
    "prettier": "^1.18.2",
    "temp-dir": "^2.0.0",
    "temp-write": "^4.0.0"
  },
  "peerDependencies": {
    "eslint": ">=6.6.0"
  }
}
